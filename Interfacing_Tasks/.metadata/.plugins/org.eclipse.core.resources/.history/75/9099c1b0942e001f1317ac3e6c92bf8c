#ifndef MCAL_EXTERNAL_INTERRUPT_EXI_REGISTER_H_
#define MCAL_EXTERNAL_INTERRUPT_EXI_REGISTER_H_

/* Define MCUCR */
#define MCUCR   		*((volatile u8*)0x55)
/* Define MCUCR Interrupt PINS */
#define MCUCR_ISC11  	3
#define MCUCR_ISC10  	2
#define MCUCR_ISC01  	1
#define MCUCR_ISC00  	0
/* Define MCUCSR */
#define MCUCSR   		*((volatile u8*)0x54)
/* Define MCUCSR Interrupt PINS */
#define MCUCSR_INT1		7
#define MCUCSR_INT0		6
#define MCUCSR_INT2		5
/* Define GICR */
#define GICR   			*((volatile u8*)0x5B)
/* Define GICR Interrupt PINS */
#define MCUCSR_INT2		6
/* Define GIFR */
#define GIFR   			*((volatile u8*)0x5A)






#endif /* MCAL_EXTERNAL_INTERRUPT_EXI_REGISTER_H_ */
